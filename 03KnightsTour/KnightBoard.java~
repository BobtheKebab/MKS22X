public class KnightsBoard {
    
    public int[][] board;
    private String boardState;
    
    public KnightBoard (int rows, int cols) {
	board = int[rows][cols];
    }

    public void updateBoardState () {
	boardState = "";
	for (int[] ary : board) {
	    for (int num : ary) {
		if (num < 10) {
		    boardState += " " + num;
		} else {
		    boardState += num;
		}
	    }
	    boardState += "\n";
	}
    }

    public String toString () {
	updateBoardState();
	return boardState;
    }

    public static void main (String[] args) {

	
